{
    "workbench.iconTheme": "vscode-great-icons",
    "editor.tabSize": 4,
    "window.menuBarVisibility": "default",
    "vsicons.dontShowNewVersionMessage": true,
    "material-icon-theme.showUpdateMessage": false,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "vsicons.projectDetection.autoReload": true,
    "gitlens.keymap": "alternate",
    "gitlens.gitExplorer.files.layout": "tree",
    "[python]": {
        "editor.rulers": [
            79,
            120
        ]
    },
    "git.autofetch": true,
    "gitlens.historyExplorer.enabled": false,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#00BCD4",
        "list.activeSelectionForeground": "#00BCD4",
        "list.inactiveSelectionForeground": "#00BCD4",
        "list.highlightForeground": "#00BCD4",
        "scrollbarSlider.activeBackground": "#00BCD450",
        "editorSuggestWidget.highlightForeground": "#00BCD4",
        "textLink.foreground": "#00BCD4",
        "progressBar.background": "#00BCD4",
        "pickerGroup.foreground": "#00BCD4",
        "tab.activeBorder": "#00BCD4",
        "notificationLink.foreground": "#00BCD4",
        "editorWidget.border": "#00BCD4"
    },
    "materialTheme.accentPrevious": "",
    "materialTheme.accent": "Cyan",
    "c-cpp-flylint.clang.executable": "clang.exe",
    "c-cpp-flylint.clang.standard": [
        "c11",
        "c++17"
    ],
    "c-cpp-flylint.cppcheck.executable": "cppcheck.exe",
    "c-cpp-flylint.cppcheck.suppressions": [
        "missingIncludeSystem"
    ],
    "c-cpp-flylint.cppcheck.standard": [
        "c11",
        "c++11"
    ],
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.includePaths": [
        "include",
        "../lib/boost_1_67_0"
    ],
    "c-cpp-flylint.excludeFromWorkspacePaths": [
        "../lib/boost_1_67_0"
    ],
    "[cpp]": {
        "editor.tabSize": 2,
        "editor.rulers": [
            80
        ]
    }
}